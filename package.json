    "rxjs": "6.5.5",
    "sass": "1.22.12",
    "sass-loader": "7.1.0",
    "semantic-release": "17.0.4",
    "style-loader": "0.23.1",
    "stylelint": "8.4.0",
    "stylelint-webpack-plugin": "0.10.5",
    "svgxuse": "1.2.6",
    "ts-helpers": "1.1.2",
    "ts-loader": "5.3.3",
    "tslint": "5.15.0",
    "tslint-loader": "3.5.4",
    "typescript": "3.8.3",
    "wait-on": "3.2.0",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.2.1",
    "webpack-node-externals": "1.7.2",
    "whatwg-fetch": "3.0.0",
    "zone.js": "~0.10.2"
  },
  "dependencies": {
    "@angular/animations": "^9.1.11",
    "@carbon/utils-position": "1.1.3",
    "flatpickr": "4.6.1",
    "tslib": "^1.10.0"
  }
}

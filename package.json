    "karma-notify-reporter": "1.0.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "3.0.0",
    "node-sass": "4.10.0",
    "postcss-loader": "2.1.4",
